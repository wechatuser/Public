#Here is Q1

num = int (input ("Choose a number:"))

firstTerm = 0
secondTerm = 1

for (i in range(num)):
	if (i <= 1):
		temp = i
	else:
		temp = firstTerm + secondTerm
		firstTerm = secondTerm
		secondTerm = temp
	print(temp)




